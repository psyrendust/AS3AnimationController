package com.psyrendust.control.types{	import com.psyrendust.control.events.AnimationControllerEvent;			/**	 * The AnimationControllerType class allows you to 	 * create a AnimationControllerType object. This object	 * is used as a default set of parameters for the 	 * AnimationController.	 * 	 * @example Using AnimationControllerType in a class	 * <listing>	 * import com.psyrendust.control.AnimationController;	 * import com.psyrendust.control.types.AnimationControllerType;	 * import flash.display.MovieClip;	 * 	 * public class Main extends MovieClip	 * {	 *    private var ac:AnimationController;	 *    	 *    public function Main()	 *    {	 *       super();	 *       var mc:MovieClip = new MovieClip();	 *       mc.graphics.beginFill(0xFF0066);	 *       mc.graphics.drawCircle(200,200,100);	 *       mc.graphics.endFill();	 *       ac = new AnimationController(mc, new AnimationControllerType());	 *       addChild(ac.mc);	 *    }	 * }	 * </listing>	 * 	 * @see com.psyrendust.control.AnimationController AnimationController	 * 	 * @author lgordon@psyrendust.com	 */	public class AnimationControllerType	{		/**		 * The direction of the animation playhead.		 * 		 * @default AnimationControllerEvent.FORWARD		 * @see com.psyrendust.control.AnimationController#direction AnimationController direction property		 */		public var direction:String=AnimationControllerEvent.FORWARD;				/**		 * A Boolean value if set to <code>true</code> will 		 * bring the playhead to the first frame and stop it there.		 * 		 * @default <code>true</code>		 */		public var stopAtFirstFrame:Boolean=true;				/**		 * A Boolean value that indicates whether the AnimationController 		 * stops the playhead when it encounters a labeled key frame.		 * 		 * @default <code>true</code>		 * @see com.psyrendust.control.AnimationController#stopAtLabel AnimationController stopAtLabel property		 */		public var stopAtLabel:Boolean=true;				/**		 * The delay, in milliseconds, between timer events.		 * 		 * @default <code>33</code>		 * @see com.psyrendust.control.AnimationController#timerDelay AnimationController timerDelay property		 */		public var timerDelay:Number=33;				/**		 * The frame rate of the animation playhead.		 * 		 * @default <code>1</code>		 * @see com.psyrendust.control.AnimationController#frameRate		 */		public var frameRate:Number=1;	}}