package com.psyrendust.control.events{	/**	 * The AnimationControllerEvent class defines type properties that	 * are used for AnimationController events.	 * 	 * @author lgordon@psyrendust.com	 */	public class AnimationControllerEvent	{		/**		 * The AnimationControllerEvent.STOPPED_AT_LABEL constant 		 * defines the value of the type property of a stoppedAtLabel 		 * event object.		 */		public static const STOPPED_AT_LABEL:String = "stoppedAtLabel";				/**		 * The AnimationControllerEvent.ADDED_TO_STAGE constant 		 * defines the value of the type property of an addedToStage 		 * event object.		 */		public static const ADDED_TO_STAGE:String = "addedToStage";				/**		 * The AnimationControllerEvent.REMOVED_FROM_STAGE constant 		 * defines the value of the type property of a removedFromStage 		 * event object.		 */		public static const REMOVED_FROM_STAGE:String = "removedFromStage";				/**		 * The AnimationControllerEvent.FORWARD constant 		 * defines the value of the type property of a forward 		 * event object.		 */		public static const FORWARD:String = "forward";				/**		 * The AnimationControllerEvent.REVERSE constant 		 * defines the value of the type property of a reverse 		 * event object.		 */		public static const REVERSE:String = "reverse";	}}