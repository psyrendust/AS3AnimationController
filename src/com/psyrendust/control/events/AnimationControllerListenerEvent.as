package com.psyrendust.control.events{	/**	 * The AnimationControllerListenerEvent contains properties that specify 	 * a frame number and the corresponding label name.	 *	 * @author lgordon@psyrendust.com	 */	public class AnimationControllerListenerEvent	{		/**		 * The frame number containing the label.		 */		public var frame:int;				/**		 * The name of the label.		 */		public var name:String;				/**		 * Constructor for AnimationControllerListenerEvent instances.		 */		public function AnimationControllerListenerEvent(frame:Number, name:String):void		{			this.frame = frame;			this.name = name;		}	}}