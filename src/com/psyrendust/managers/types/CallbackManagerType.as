package com.psyrendust.managers.types{	/**	 * The CallbackManagerType class allows you to create a CallbackManagerType object.	 * 	 * @author lgordon@psyrendust.com	 */	public class CallbackManagerType	{		/**		 * The type of event.		 */		public var type:String;				/**		 * The scope that contains the callback function.		 */		public var callbackDelegate:*;				/**		 * The name of the function.		 */		public var callbackFunction:String;		/**		 * The constructor for the CallbackManagerType.		 * 		 * @param type The type of event.		 * @param callbackDelegate The scope that contains the callback function.		 * @param callbackFunction The name of the function.		 * @see com.psyrendust.managers.CallbackManager CallbackManager		 */		public function CallbackManagerType(type:String, callbackDelegate:*, callbackFunction:String):void		{			this.type=type;			this.callbackDelegate=callbackDelegate;			this.callbackFunction=callbackFunction;		}	}}